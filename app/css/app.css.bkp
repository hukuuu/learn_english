body {
	font-family: 'Open Sans', arial, sans-serif;
}

div {
	position: relative;
	margin: 0 0 20px 0;
}

div label {
	margin: 0 0 1px;
	padding: 5px 10px;
	color: #fff;
	font-size: 18px;
    font-weight: 400;
	display: block;
}

div input.form-control {
	padding: 5px 9px;
	font-size: 18px;
	border-radius: 0;
}

.form-control.close {
	display: none;
}

strong {
	display: inline-block;
	float: right;
	width: 20px;
	height: 20px;
}
